{
  "name": "jsapi-cli-vitest",
  "version": "0.0.0",
  "type": "module",
  "browser": "index.html",
  "scripts": {
    "prepare": "husky install",
    "start": "vite",
    "copy-css": "node ./tools/copy-css.js",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "prettier": "prettier --write \"src/**/*.ts?(x)\"",
    "lint": "eslint . --ext .js,.ts,.tsx --fix",
    "test": "vitest run --passWithNoTests --browser",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitest/browser": "^0.29.2",
    "c8": "^7.13.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^11.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-pwa": "^0.14.4",
    "vitest": "^0.28.5"
  },
  "dependencies": {
    "@arcgis/core": "~4.26.0"
  },
  "arcgis": {
    "type": "vite"
  }
}
